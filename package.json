    "dts-bundle": "^0.7.3",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.16",
    "terser-webpack-plugin": "^2.3.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
